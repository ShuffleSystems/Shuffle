{
  "name": "ui",
  "version": "0.1.0",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react && tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "concurrently \"tsup src/index.tsx --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "clean": "rm -rf dist",
    "lint": "eslint *.ts"
  },
  "devDependencies": {
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "eslint-config-custom": "workspace:*",
    "next": "13.0.2",
    "react": "18.2.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.2.2",
    "tsconfig": "workspace:*",
    "tsup": "^6.4.0",
    "typescript": "4.8.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
